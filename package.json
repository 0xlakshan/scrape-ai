{
  "name": "scrape-kit",
  "version": "0.3.0",
  "description": "TypeScript web scraping SDK with AI-powered extraction",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup packages/sdk/src/index.ts --format cjs,esm --dts",
    "dev": "tsup packages/sdk/src/index.ts --format cjs,esm --dts --watch",
    "serve": "tsx packages/api/index.ts",
    "postinstall": "npx playwright install chromium",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "scraping",
    "ai",
    "playwright",
    "sdk"
  ],
  "author": "Lakshan Perera",
  "license": "MIT",
  "dependencies": {
    "ai": "^6.0.3",
    "express": "^4.18.2",
    "playwright": "^1.40.0",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "zod": "^3.22.0"
  },
  "peerDependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "zod": "^3.22.0"
  }
}
