{
  "name": "scrape-kit",
  "version": "0.2.0",
  "description": "TypeScript web scraping SDK with AI-powered transformation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "serve": "tsx src/api.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": ["scraping", "ai", "playwright", "sdk"],
  "author": "Lakshan Perera",
  "license": "MIT",
  "dependencies": {
    "ai": "^6.0.3",
    "express": "^4.18.2",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/anthropic": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/google": { "optional": true },
    "@ai-sdk/openai": { "optional": true },
    "@ai-sdk/anthropic": { "optional": true }
  }
}
